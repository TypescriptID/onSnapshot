    "start": "npm run build && npm run serve",
    "build": "ng build --aot --prod && ng build --prod --app 1 --output-hashing=false && webpack --config webpack.server.config.js --progress --colors",
    "build:firebase": "npm run build && rm -rf ../public ../functions/dist && cp -r dist/browser ../public && mkdir ../functions/dist && cp dist/browser/index.html ../functions/dist && cp dist/firebase.js ../functions/dist && rm ../public/index.html",
    "build:flex": "npm run build && rm -rf ../appengine/dist && cp -r dist ../appengine",
    "serve": "node dist/server"
  },
